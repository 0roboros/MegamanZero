        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Check Alive
if global.life2 &lt; 0.5
{
instance_change(Gray_Death,1)
image_blend = c_green
sound_play(ZeroDie)
}

if cd &gt; 0{
cd -= 1;
}
if cd = 1{
sprite_index = Gray
}
//Check Ground
if place_meeting(x,y+3,wall) = true
{
ground = 1
djump = 1
walljump = 0
}
else
{
gravity = 0.3
ground = 0
}

//Check Ceiling
if place_meeting(x,y-3,wall) = true
{
block = 1
}
else
{
block =0
}

//Check Right
if place_meeting(x + 6, y,wall) = false
{
mr = 1
walljright = 0
}
else
{
mr = 0    

//Check WallJump Right

    if ground = 0
    {
    walljright = 1
    }
}


//Check Left
if place_meeting(x - 6, y, wall) = false
{
ml = 1
walljleft = 0
}
else
{
ml = 0

//Check WallJump Left

    if ground = 0
    {
    walljleft = 1
    }
}

//check Wallslide
if place_meeting(x+6,y,wall) = false and place_meeting(x-6,y,wall) = false or (ground = 1)
{
wallslide = 0
}

//Sprite Changes

if cd = 0
{
    if Animation_End &lt; 1
        {
        if wallslide = 0
        {
        if vspeed &gt; 0 and ground = 0
        sprite_index = Gray_Fall
        }
        if vspeed &lt; 0 and ground = 0
        sprite_index = Gray_Jump
        }
}

//Check WallJumping

if leftj = 1
{
x = x - 4
flowcount = flowcount + 1
sprite_index = Gray_WallJump
}

if rightj = 1
{
x = x + 4
flowcount = flowcount + 1
sprite_index = Gray_WallJump
image_xscale = -1
}

//Stop Walljump

if flowcount &gt; 7
{
rightj = 0
leftj = 0
}
//Slash Delay
if slash = 1
{
slashdelay = slashdelay + 1
    if slashdelay &gt; 14
    {
    slashdelay = 0
    slash = 0
    sprite_index = Gray
    }
}

//Slide
if slide &gt; 0
{
slide = slide - 1
if hsp = 1
hspeed = 3
if hsp = -1
hspeed = -3
}

if slide &lt; 1
hspeed = 0

//hit

if hit = 1{
if righthit = 1{
    if place_meeting(x - 16,y,wall) = false and place_meeting(x - 16,y,airwall) = false
    {
    x = x -16
    }
sprite_index = Gray_Hit
    righthit = 0
}
if lefthit = 1{
    if place_meeting(x + 16,y,wall) = false and place_meeting(x + 16,y,airwall) = false
    {
    x = x +16
    }
sprite_index = Gray_Hit
    lefthit = 0
}
Animation_End = 16
Invulnerable = 60
hit = 0
sound_play(ZeroHit)
}
if Animation_End &gt; 0
{
Animation_End -= 1
    if Animation_End &lt; 1
    {
        if ground = 1
        sprite_index = Gray
    }
}

if Invulnerable &gt; 0
{
Invulnerable = Invulnerable - 1
    if Invulnerable &gt; 10
    {
    image_alpha = (irandom(4) * 0.2)
    }   


if Invulnerable &lt; 11
image_alpha = 1
}

//Limit vspeed

if vspeed &gt; 8
vspeed = 8
</string>
